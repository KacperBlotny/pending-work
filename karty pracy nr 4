// ZADANIE 

//1

long double pot(int o, int p)
{
    long double pomocnicza = 1;
    if(p != 0);
   {
    for(int i=0; i<p; i++)
    {
        pomocnicza = pomocnicza * o;
    }

   }
    return pomocnicza;
}

//2

int potmin(int y, int u)
{
    float pomocnicza = 1;
    if( u != 0 )
    {
        for(int i=0; i<u; i++)
        {
            pomocnicza = pomocnicza * y;
            
            return pomocnicza;
        }
        
        if(u<0)
        {
            for(int i=0; i<u; i++)
            {
                pomocnicza = pomocnicza * y;
            }
            return 1/pomocnicza;
            
        }
        
    }
}






